<?xml version="1.0"?>
<form>
    <notebook colspan="6">
        <page string="General" id="general">
            <label name="file"/>
            <field name="file"/>
            <label name="company"/>
            <field name="company"/>
            <newline/>
            <label name="status"/>
            <field name="status"/>
            <separator string="Datos Personales" colspan="6" id='datos_personales'/>
            <label name="party"/>
            <field name="party"/>
            <newline/>
            <label name="first_name"/>
            <field name="first_name"/>
            <label name="last_name"/>
            <field name="last_name"/>
            <label name="gender"/>
            <field name="gender"/>
            <label name="dni"/>
            <field name="dni"/>
            <label name="nationality"/>
            <field name="nationality" widget="selection"/>
            <label name="marital_status"/>
            <field name="marital_status"/>
            <label name="birthdate"/>
            <field name="birthdate"/>
            <separator string="Incorporacion" colspan="6" id='incorporacion_coop'/>
            <label name="incorporation_date"/>
            <field name="incorporation_date"/>
            <label name="leaving_date"/>
            <field name="leaving_date"/>
            <label name="payed_quotes"/>
            <field name="payed_quotes"/>
            <separator string="Legal Documents" colspan="6" id="legal_documents"/>
            <label name="proposal_letter"/>
            <field name="proposal_letter"/>
            <label name="proof_tax"/>
            <field name="proof_tax"/>
            <label name="meeting_date_of_incoroporation"/>
            <field name="meeting_date_of_incoroporation"/>
        </page>
        <page string="Permit and Vacation" id="vacations">
            <label name="vacation_days"/>
            <field name="vacation_days"/>
            <newline/>
            <field name="vacation" colspan="2"/>
        </page>
        <page string="Sanctions" id="sanctions">
            <field name="sanction"/>
        </page>
        <page string="Meetings" id= "meetings">
            <field name="meeting"/>
        </page>
        <page string="Recibos de Socio" id= "recibos">
            <field name="recibos"/>
        </page>
    </notebook>
</form>
